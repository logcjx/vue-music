{
  "presets": [
    ["env", { "modules": false }],
    "stage-2"
  ],
  "plugins": [
    ["component", [
      {
        "libraryName": "mint-ui",
        "style": true
      }
    ]],
    "transform-runtime", 
    "lodash",
    "syntax-dynamic-import"
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
